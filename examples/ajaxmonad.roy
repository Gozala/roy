type Request = {url: String, payload: String}

let ajaxRequest = {
  return: \x -> x
  bind: \(x : Request) f ->
    $.get x.url x.payload f
}

let v = (do ajaxRequest.bind
  value <- {url: '/examples/helloworld.roy', payload: 'stuff'}
  console.log value
  ajaxRequest.return value
)

console.log v
